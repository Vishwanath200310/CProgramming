#include<stdio.h>

int func(char* s1[20]){
         int occur = 0;
         for (int i = 0; i<21 ; i++)
         {
            if (s1[i] == 'o')
            {
                occur++;
            }
         }
         return occur++;
}
int main(){
    char s1[20] = "helloworld";
    printf("%d", func(s1));
    return 0;
}